{
  "name": "Fact CHECK24",
  "description": "Live video fact-checking tool as a browser extension.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabCapture", "activeTab", "storage", "sidePanel"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_title": "Click to open extension"
  },
  "side_panel": {
    "default_path": "side_panel/side_panel.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  }
}
